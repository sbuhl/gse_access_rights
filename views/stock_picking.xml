<odoo>
  <data>
    <record id="gse_access_right_picking_inherit" model="ir.ui.view">
      <field name="name">gse_access_right.picking.view.form.inherit</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form"/>
      <field name="priority">99</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//header" position="replace">
              <header>
                    <button name="action_confirm" modifiers="{'invisible': [('show_mark_as_todo', '=', False)]}" string="Mark as Todo" type="object" class="oe_highlight" groups="gse_access_rights.group_stock_validator" data-hotkey="x"/>
              <button name="action_assign" modifiers="{'invisible': [('show_check_availability', '=', False)]}" string="Check Availability" type="object" class="oe_highlight" groups="gse_access_rights.group_stock_validator"/>
              <button name="button_validate" modifiers="{'invisible': ['|', ('state', 'in', ('waiting','confirmed')), ('show_validate', '=', False)]}" string="Validate" type="object" class="oe_highlight" groups="gse_access_rights.group_stock_validator" data-hotkey="v"/>
              <button name="button_validate" modifiers="{'invisible': ['|', ('state', 'not in', ('waiting', 'confirmed')), ('show_validate', '=', False)]}" string="Validate" type="object" groups="gse_access_rights.group_stock_validator" class="o_btn_validate" data-hotkey="v"/>
              <!-- Removed the problematic button here -->
              <widget name="signature" string="Sign" highlight="1" modifiers="{'invisible': ['|', '|', ('id', '=', False), ('picking_type_code', '!=', 'outgoing'), ('state', '!=', 'done')]}" full_name="partner_id" groups="stock.group_stock_sign_delivery"/>
              <widget name="signature" string="Sign" modifiers="{'invisible': ['|', '|', ('id', '=', False), ('picking_type_code', '!=', 'outgoing'), ('state', '=', 'done')]}" full_name="partner_id" groups="stock.group_stock_sign_delivery"/>
              <button name="do_print_picking" string="Print" groups="gse_access_rights.group_stock_validator" type="object" modifiers="{'invisible': [('state', '!=', 'assigned')]}" data-hotkey="o"/>
              <button string="Print Labels" type="object" name="action_open_label_layout"/>
              <button name="425" string="Print" modifiers="{'invisible': [('state', '!=', 'done')]}" type="action" groups="gse_access_rights.group_stock_validator" data-hotkey="o"/>
              <button name="441" string="Return" modifiers="{'invisible': [('state', '!=', 'done')]}" type="action" groups="gse_access_rights.group_stock_validator" data-hotkey="k"/>
              <button name="do_unreserve" type="object" string="Unreserve" groups="gse_access_rights.group_stock_validator"/>
              <button name="button_scrap" type="object" string="Scrap" modifiers="{'invisible': ['|', ('state', '=', 'cancel'), ('state', '=', 'draft')]}" data-hotkey="y"/>
              <button name="action_toggle_is_locked" modifiers="{'invisible': ['|', ('state', 'in', ('draft','cancel')), ('is_locked', '=', False)]}" string="Unlock" groups="stock.group_stock_manager" type="object" help="If the picking is unlocked you can edit initial demand (for a draft picking) or done quantities (for a done picking)." data-hotkey="l"/>
              <button name="action_toggle_is_locked" modifiers="{'invisible': [('is_locked', '=', True)]}" string="Lock" groups="stock.group_stock_manager" type="object" data-hotkey="l"/>
              <field name="state" widget="statusbar" statusbar_visible="draft,confirmed,assigned,done"/>
              <button name="action_cancel" modifiers="{'invisible': [('state', 'not in', ('assigned', 'confirmed', 'draft', 'waiting'))]}" string="Cancel" groups="gse_access_rights.group_stock_validator" type="object" data-hotkey="z"/>
            </header>
          </xpath>
        </data>
      </field>
    </record>
  </data>
</odoo>